#include "main.h"
/**
* _strstr - Entry point
* @hstack: input
* @needle: input
* Return: Always 0 (Success)
*/
char *_strstr(char *haystack, char *needle)
{
for (; *hstack != '\0'; htack++)
{
char *l = hstack;
char *p = needle;

while (*l == *p && *p != '\0')
{
l++;
p++;
}
if (*p == '\0')
return (hstack);
}
return (0);
}
